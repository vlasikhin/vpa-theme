		"editor.selectionBackground": "#dae7f7",
		"editorCursor.background": "#f6f6f6",
		"editorCursor.foreground": "#509aff",
		"editorIndentGuide.activeBackground": "#dae7f7",
		"editorLineNumber.activeForeground": "#636363",
		"editorLineNumber.foreground": "#b6b6b6",
		"list.activeSelectionBackground": "#dcdcdc",
		"list.activeSelectionForeground": "#4f4f4f",
		"list.activeSelectionIconForeground": "#f6f6f6",
		"list.focusAndSelectionOutline": "#4f4f4f",
		"list.focusOutline": "#4f4f4f",
		"notifications.border": "#4f4f4f",
		"notificationToast.border": "#4f4f4f",
		"panel.border": "#4f4f4f",
		"panelTitle.activeBorder": "#dae7f7",
		"sideBar.background": "#f6f6f6",
